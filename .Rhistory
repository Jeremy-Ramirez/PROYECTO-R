data=read.csv(url("http://archive.ics.uci.edu/ml/machine-learning-databases/wine/wine.data"), header = FALSE)
names(data)=c('Class','Alcohol','Malic acid','Ash','Alcalinity of ash','Magnesium','Total phenols','Flavanoids','Nonflavanoid phenols','Proanthocyanins','Color intensity','Hue','OD280/OD315','Proline')
library(dplyr)
library(datasets)
fc1=data %>% filter(data$Class==1)
fc1
hist(fc1$Alcohol)
summary(fc1$Alcohol)
boxplot(fc1$Alcohol)
boxplot(fc1$Alcohol,horizontal = TRUE)
boxplot(fc1$Alcohol~fc1$Class,horizontal = TRUE)
boxplot(fc1$Alcohol~fc1,horizontal = TRUE)
boxplot(fc1$Alcohol~fc1$Class,horizontal = TRUE)
boxplot(fc1$`Malic acid`~fc1$Class,horizontal = TRUE)
sd(fc1$Alcohol)
library(fdth)
tb1=fdt(fc1$Alcohol,breaks = "sturges")
tb1=fdt(fc1$Alcohol,breaks = "Sturges")
tb1
tb1=fdt(fc1$Class,breaks = "Sturges")
tb1
tbt=fdt(data$Class)
tbt
plot(x = fc1$Class)
plot(x = data$Class)
barplot(x = data$Class)
barplot(prop.table(table(fc1$Class)))
barplot(prop.table(table(data$Class)))
barplot(prop.table(table(data$Class)),col=c("orange","blue"))
barplot(prop.table(table(data$Class)),col=c("orange","blue"),legend.text=c("Vino1","Vino2","Vino3")))
barplot(prop.table(table(data$Class)),col=c("orange","blue"),legend.text=c("Vino1","Vino2","Vino3"))
barplot(prop.table(table(data$Class)),col=c("orange","blue"),legend.text=c("Vino1","Vino2","Vino3"),ylim=c(0,0.8),main="CLASE DE VINOS")
barplot(prop.table(table(data$Class)),col=c("orange","blue","red"),legend.text=c("Vino1","Vino2","Vino3"),ylim=c(0,0.8),main="CLASE DE VINOS")
barplot(prop.table(table(data$Class)),col=c("yellow","blue","red"),legend.text=c("Vino1","Vino2","Vino3"),ylim=c(0,0.8),main="CLASE DE VINOS")
